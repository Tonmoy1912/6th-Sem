INSERT INTO EMP_1133 VALUES ('12','TONMOY BISWAS 12','1','10','MALE','KOLKATA','KOLKATA','WEST BENGAL','700032','1000',TO_DATE('2021-12-19', 'yyyy/mm/dd'));

INSERT INTO EMP_1133 VALUES ('13','NEW_MEMBER 12','1','1','FEMALE','KOLKATA','KOLKATA','WEST BENGAL','700032','9000',TO_DATE('2021-12-19', 'yyyy/mm/dd'));

18/
delete from emp_1133 where desig_code not in ( select desig_code from designation_1133);

19/
select emp_name from emp_1133 where sex='FEMALE' and basic>(select avg(basic) from emp_1133 e where e.dept_code=dept_code);

